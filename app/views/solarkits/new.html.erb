<div class="container">
  <h1>Create a solarkit</h1>
<%= simple_form_for @solarkit do |f| %>
    <%= f.input :power %>
    <%= f.input :min_production %>
    <%= f.input :max_production %>
    <%= f.input :price %>
    <%= f.input :installation_price %>
    <%= f.button :submit %>
  <% end %>
</div>
